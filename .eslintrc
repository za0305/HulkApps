{
  "extends": [
    "plugin:shopify/esnext",
    "plugin:shopify/jquery",
    "plugin:vue/recommended"
  ],
  "env": {
    "jest": true
  },
  "rules": {
    "shopify/strict-component-boundaries": 0,
    "shopify/prefer-early-return": 0,
    "no-alert": 0,
    "no-console": 0,
    "no-undefined": 0,
    "no-undef": 0,
    "no-unused-vars": 0,
    "no-unused-expressions": 0,
    "no-mixed-operators": 0,
    "no-new": 0,
    "no-shadow": 0,
    "no-param-reassign": 0,
    "no-nested-ternary": 0,
    "no-useless-escape": 0,
    "no-sequences": 0,
    "no-empty-function": 0,
    "no-process-env": 0,
    "no-negated-condition": 0,
    "no-dupe-keys": 0,
    "no-misleading-character-class": 0,
    "prefer-rest-params": 0,
    "babel/no-invalid-this": 0,
    "babel/object-curly-spacing": 0,
    "import/extensions": 0,
    "import/no-cycle": 0,
    "import/no-unresolved": ["error", {"ignore": ["^jquery"]}],
    "max-len": 0,
    "camelcase": 0,
    "id-length": 0,
    "line-comment-position": 0,
    "consistent-this": 0,
    "lines-around-comment": 0,
    "consistent-return": 0,
    "prefer-object-spread": 0,
    "require-atomic-updates": 0
  },
  "globals": {
    "document": true,
    "window": true,
    "Shopify": true,
    "theme": true
  }
}
