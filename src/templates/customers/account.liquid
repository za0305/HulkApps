{% assign collection = collections[settings.choose_collection] %}
{% assign productLimit = settings.product_limit %}

<section class="section-content mb-4" data-template="account">
   <div class="account-heading-wrapper">
     <div class="container">
        <ul class="list-inline mb-0 nav nav-tabs border-0 justify-content-center account-link mb-6" id="myTab" role="tablist" data-tablist>
          <li class="list-inline-item mr-0 my-2 my-md-3">
            <a class="px-3 font-barlow font-size-sm font-weight-medium ls-1 active" id="account" data-toggle="tab" href="#account-tab" role="tab" aria-controls="account" aria-selected="true" @click="_scrollTop();">My Account</a>
          </li>
          <template v-if="!rechargeDashboard.subscriptions.length == 0">
            <li class="list-inline-item mr-0 my-2 my-md-3">
              <a class="px-3 font-barlow font-size-sm font-weight-medium ls-1" id="subscriptions-tab" data-toggle="tab" href="#subscriptions" role="tab" aria-controls="subscriptions" aria-selected="false" data-subscriptions-tab  @click="_scrollTop();">My Subscriptions</a>
            </li>
            <li class="list-inline-item mr-0 my-2 my-md-3">
              <a class="px-3 font-barlow font-size-sm font-weight-medium ls-1" href="/tools/recurring/login/">Billing Information</a>
            </li>
          </template>
          {% comment %}
          <li class="list-inline-item mr-0 my-2 my-md-3">
            <a class="px-3 font-barlow font-size-sm font-weight-medium ls-1" id="refer-friend-tab"  href="https://owlcrate.com/pages/refer">Refer A Friend</a>
          </li>
          {% endcomment %}
          <li class="list-inline-item mr-0 my-2 my-md-3">
            <a class="px-3 font-barlow font-size-sm font-weight-medium ls-1" id="help-tab" data-toggle="tab" href="#help" role="tab" aria-controls="help" aria-selected="false"  @click="_scrollTop();">Help</a>
          </li>
          <li class="list-inline-item mr-0 my-2 my-md-3"><a href="/account/logout" class="px-3 font-barlow font-size-sm font-weight-medium ls-1">Sign out</a></li>
        </ul>
         <div class="tab-content" id="myTabContent">
           <div class="tab-pane active" id="account-tab" role="tabpanel" aria-labelledby="account">
             <div class="row justify-content-center mb-5">
               <div class="col-12 col-lg-10 heading-wrapper text-center">
                 <h2>Welcome To Your Account Dashboard!</h2>
                 <p>From here you can do a lot of things; you can change your account details, track your orders, review your order history, saved information and access your membership account. If you're a subscription holder, make your way over to the "My Subscriptions" section to make any and all changes to your subscription. If you have any questions, comments or feedback, reach out to our team at <a href="mailto:support@owlcrate.com" class="text-primary">support@owlcrate.com</a></p>
               </div>
             </div>
             <div class="account-content">
               <div class="row">
                 <div class="col-12 col-xl-3 my-3 my-xl-4">
                   {% comment %}
                   <div class="account-info">
                     <h5 class="mb-3">Account Details</h5>
                     <p class="mb-2 mb-lg-4" data-customer-name>{{ customer.name }} <!-- (<a href="#" data-toggle="modal" data-target="#modal-edit-details">edit</a>)--> </p>
                     <p class="mb-2 mb-lg-4 account-email">{{ customer.email }}</p>
                     <p class="mb-2 mb-lg-4">Password:  ******   (<a href="/account/login#recover">reset</a>)</p>
                     <p class="mb-0">Total Orders: {{ customer.orders_count }}</p>
                   </div>
                   {% endcomment %}
                    <div class="account-info border p-2">
                      <h5 class="mb-3 text-primary font-barlow">Account Details</h5>
                      <p class="mb-1 mb-lg-2 font-weight-bold text-capitalize font-barlow" data-customer-name>{{ customer.name }}</p>
                      <p class="mb-1 account-email  font-barlow">{{ customer.email }}</p>
                      <p class="mb-0"><span class="font-weight-bold  font-barlow">Total Orders:</span> {{ customer.orders_count }}</p>
                    </div>
                    <div class="account-info mt-3 border p-2">
                      <h5 class="mb-3 text-primary font-barlow">Account Options</h5>
                      <p class="mb-1  font-barlow"><a href="/account/login#recover" style="color: blue;">Reset Password</a></p>
                      <template v-if="!rechargeDashboard.subscriptions.length == 0">
                        <p class="mb-1">
                          <a class="font-barlow font-weight-normal" id="billing-tab"
                             href="/tools/recurring/login/"
                             aria-controls="billing-info" aria-selected="false" style="color: blue;">View/Edit Billing Info</a>
                        </p>
                      </template>
                    </div>
                 </div>
                 <div class="col-12 col-xl-9 my-3">
                   <div class="account-subcription pl-xl-3">

                    <template v-if="!rechargeDashboard.isgetsubscription">
                       <div class="d-flex flex-column align-items-center justify-content-center">
                         <div class="my-3">
                           <hollow-dots-spinner
                             :size="60"
                             :color="'#26bcb4'"
                           />
                         </div>
                         <p class="small text-gray-700">Please Wait...</p>
                       </div>
                     </template>
                     <template v-else>

                        {% comment %} Claim gift {% endcomment %}
                        <div class="claim-gift-wrap" v-if="claimGift.giftFound">
                            <div class="giftbox-custom-wrap-account claim-head">
                                <div class="claim-head-wrap d-sm-flex text-center flex-wrap align-items-center justify-content-between mb-3">
                                    <h5 class="mb-2 mb-sm-0">Your gifts</h5>
                                    <a href="#" class="claim-link font-barlow font-size-sm font-weight-medium text-uppercase ls-1 text-primary" :disabled="isLoading" @click.prevent="_checkAddressEnabled($event, 'multiple')" data-claim="multiple">
                                        <template v-if="isLoading && loadingEvent == 'claimAllGift'">
                                            <div class="text-center float-right">
                                                <hollow-dots-spinner
                                                :animation-duration="1000"
                                                :size="60"
                                                :color="'#26bcb4'"
                                                />
                                            </div>
                                        </template>
                                        <template v-else>
                                          Claim All Gifts
                                        </template>
                                    </a>
                                </div>
                                <div class="subscription-order-table mb-5">
                                  <div class="subscription-header bg-secondary py-2">
                                    <div class="px-md-3">
                                      <div class="row align-items-center">
                                        <div class="col-9 col-md-5"><p class="mb-0 font-barlow font-size-sm font-weight-medium text-white text-uppercase">Gift item</p></div> 
                                        <div class="d-none d-md-block col-md-4"><p class="mb-0 font-barlow font-size-sm font-weight-medium text-white text-uppercase">Sent by</p></div> 
                                        <div class="col-3 col-md-2 text-right"><p class="mb-0 font-barlow font-size-sm font-weight-medium text-white text-uppercase"></p></div>
                                      </div>
                                    </div>
                                  </div>
                                  <div class="subscription-body border border-secondary">
                                    <div v-if="claimGift.subscriptionGiftList.length > 0" v-for="gOrder in claimGift.subscriptionGiftList" class="subscription-item px-md-3">
                                      <div class="row align-items-center accordion" :id="'accordionExample'+gOrder.gift_id">

                                        <div class="col-5 col-md-5">
                                          <div class="d-flex align-items-center">
                                            <div class="d-md-none mr-3" :id="'headingOne'+gOrder.gift_id">
                                              <button class="btn btn-link px-0 py-2 border-0 btn-collapseProduct collapsed" type="button" data-toggle="collapse" :data-target="'#collapse_'+gOrder.gift_id" aria-expanded="true" :aria-controls="'#collapse_'+gOrder.gift_id">
                                                <i class="ion ion-ios-add h5 mb-0"></i>
                                                <i class="ion ion-ios-remove h5 mb-0"></i>
                                              </button>
                                            </div>
                                            <div class="d-flex align-items-center">
                                              <div class="subscription-img mr-3 mr-lg-4">
                                                <img class="img-fluid" :src="_getSizedImageUrl(gOrder.product.image.src, 'small')"/>
                                              </div>
                                              <div class="subscription-item-text d-none d-md-block">
                                                <h6 class="mb-0" v-text="gOrder.product.title"></h6>
                                                <p class="mb-0" v-if="gOrder.gift_type == 'normal'" >Gift box</p>
                                                <p class="mb-0" v-else >Gift subscription monthly</p>

                                              </div>
                                            </div>
                                          </div>

                                        </div>

                                        <div class="col-4 col-md-4 d-none d-md-block">
                                          <h6 class="sent-name mb-0" v-html="gOrder.sender_name"></h6>
                                          <p class="sent-email mb-0" v-if="gOrder.sender_email" v-html="gOrder.sender_email"></p>
                                          <p class="sent-msg mb-0" v-if="gOrder.gift_note" v-html="gOrder.gift_note"></p>
                                        </div>

                                        <div class="col-7 col-md-3 text-left text-md-right">
                                          <div class="subscription-item-text mb-3 d-md-none">
                                            <h6 class="mb-0" v-text="gOrder.product.title"></h6>
                                            <p class="mb-0" v-if="gOrder.gift_type == 'normal'" >Gift box</p>
                                            <p class="mb-0" v-else >Gift subscription monthly</p>

                                          </div>
                                          <button type="submit" class="btn btn-sm btn-primary" :disabled="isLoading" @click.prevent="_checkAddressEnabled($event,'single', gOrder.gift_id)">
                                            <template v-if="isLoading && loadingEvent == 'activate_'+gOrder.gift_id">
                                                <span class="spinner-border spinner-border-sm" role="status" aria-hidden="true"></span> {{ 'general.wait' | t }}
                                            </template>
                                            <template v-else>
                                                Claim gift
                                            </template>
                                          </button>
                                        </div>

                                        <div :id="'collapse_'+gOrder.gift_id" class="collapse col-12 d-md-none" :aria-labelledby="'headingOne'+gOrder.gift_id" :data-parent="'#accordionExample'+gOrder.gift_id">
                                          <div class="border-top border-secondary mt-5 pt-5">
                                            <div class="row mb-2">
                                              <div class="col-4">
                                                <p class="mb-0 font-barlow font-weight-medium text-uppercase">Sent by</p>
                                              </div>
                                              <div class="col-8">
                                                 <h6 class="sent-name mb-0" v-html="gOrder.sender_name"></h6>
                                                  <p class="sent-email mb-0" v-if="gOrder.sender_email" v-html="gOrder.sender_email"></p>
                                                  <p class="sent-msg mb-0" v-if="gOrder.gift_note" v-html="gOrder.gift_note"></p>
                                              </div>
                                            </div>
                                          </div>
                                        </div>

                                      </div>
                                    </div>

                                  </div>

                                </div>
                            </div>
                        </div>

                        {% comment %} Recharge Subscriptions {% endcomment %}
                        <div class="account-subcription-header d-sm-flex text-center flex-wrap align-items-center justify-content-between mb-3">
                          <h5 class="mb-2 mb-sm-0">Subscriptions</h5>
                          <template v-if="!rechargeDashboard.subscriptions.length == 0">
                            <a href="#" class="font-barlow font-size-sm font-weight-medium text-uppercase ls-1 text-primary" @click.prevent="_changeTab();">Manage Subscriptions<i class="ion ion-ios-arrow-forward ml-2"></i></a>
                          </template>
                        </div>
                       <template v-if="!rechargeDashboard.subscriptions.length == 0">
                         <div class="subscription-order-table mb-5">
                           <div class="subscription-header bg-secondary py-2">
                             <div class="px-md-3">
                               <div class="row align-items-center">
                                 <div class="col-9 col-md-4">
                                   <p class="mb-0 font-barlow font-size-sm font-weight-medium text-white text-uppercase">Order</p>
                                 </div>
                                 <div class="d-none d-md-block col-md-2">
                                   <p class="mb-0 font-barlow font-size-sm font-weight-medium text-white text-uppercase">Status</p>
                                 </div>
                                 <div class="col-4 col-md-2 d-none d-md-block">
                                   <p class="mb-0 font-barlow font-size-sm font-weight-medium text-white text-uppercase">TERM</p>
                                 </div>
                                 <div class="col-4 col-md-2 d-none d-md-block">
                                   <p class="mb-0 font-barlow font-size-sm font-weight-medium text-white text-uppercase">Renews</p>
                                 </div>
                                 <div class="col-3 col-md-2 text-right">
                                   <p class="mb-0 font-barlow font-size-sm font-weight-medium text-white text-uppercase">total</p>
                                 </div>
                               </div>
                             </div>
                           </div>
                           <div class="subscription-body border border-secondary">
 
                             <div class="subscription-item px-md-3" v-for="(order, index) in rechargeDashboard.subscriptions" :data-index="index" :data-variant-id="order.shopify_variant_id" :data-product-id="order.shopify_product_id">                              
                               <div class="row align-items-center accordion" :id="'accordionExample_'+order.id">
                                 <div class="col-9 col-md-4">
                                   <div class="d-flex align-items-center">
                                     <div class="d-md-none mr-3" :id="'headingOne_'+order.id">
                                       <button class="btn btn-link px-0 py-2 border-0 btn-collapseProduct collapsed" type="button" data-toggle="collapse" :data-target="'#collapse_'+order.id" aria-expanded="true" :aria-controls="'collapse_'+order.id">
                                         <i class="ion ion-ios-add h5 mb-0"></i>
                                         <i class="ion ion-ios-remove h5 mb-0"></i>
                                       </button>
                                     </div>
                                     <div class="d-flex align-items-center">
                                       <div class="subscription-img mr-3 mr-lg-4">
                                         <template v-if="order.image">
                                           <img :src="order.image" :alt="order.product_title" class="img-fluid">
                                         </template>
                                         <template v-else>
                                           <span class="blank-img d-block w-100" >
                                             {{ 'product-1' | placeholder_svg_tag: 'img' }}
                                           </span>
                                         </template>
                                       </div>
                                       <div class="subscription-item-text">
                                         <h6 class="mb-0" v-text="'#'+order.id"></h6>
                                         <p class="mb-0" v-text="order.product_title"></p>
                                       </div>
                                     </div>
                                   </div>
                                 </div>
                                 <div class="col-4 col-md-2 d-none d-md-block">
                                   <p class="mb-0" v-text="order.status"></p>
                                 </div>
                                 <div class="col-4 col-md-2 d-none d-md-block">
                                   <p class="mb-0" v-text="order.variant_title"></p>
                                 </div>
                                 <div class="col-4 col-md-2 d-none d-md-block">
                                   <p class="mb-0" v-if="order.next_charge_scheduled_at" v-text="_formatDateRecharge(order.next_charge_scheduled_at)"></p>
                                   <p class="mb-0" v-else>--</p>
                                 </div>
                                 <div class="col-3 col-md-2 text-right">
                                   <p class="mb-0" v-html="_formatMoney(order.price*100)"></p>
                                 </div>
                                 <div :id="'collapse_'+order.id" class="collapse col-12 d-md-none" :aria-labelledby="'headingOne_'+order.id" :data-parent="'#accordionExample_'+order.id">
                                   <div class="border-top border-secondary mt-5 pt-5">
                                     <div class="row mb-2">
                                       <div class="col-6"><p class="mb-0 font-barlow font-weight-medium text-uppercase">Status</p></div>
                                       <div class="col-6"><p class="mb-0" v-text="order.status"></p></div>
                                     </div>
                                     <div class="row mb-2">
                                       <div class="col-6"><p class="mb-0 font-barlow font-weight-medium text-uppercase">TERM</p></div>
                                       <div class="col-6"><p class="mb-0" v-text="order.variant_title"></p></div>
                                     </div>
                                     <div class="row mb-0">
                                       <div class="col-6"><p class="mb-0 font-barlow font-weight-medium text-uppercase">Renews</p></div>
                                       <div class="col-6"><p class="mb-0" v-if="order.next_charge_scheduled_at" v-text="_formatDateRecharge(order.next_charge_scheduled_at)"></p>
                                         <p class="mb-0" v-else>--</p></div>
                                     </div>
                                   </div>
                                 </div>
                               </div>
                             </div>

                           </div>
                         </div>
                       </template>
                       <template v-else>
                         <p class="ls-1 mb-0">You don't have any subscriptions. <a href="/pages/subscribe" class="line-hover-primary font-weight-semi-bold">Start Shopping</a></p>
                       </template>
                     </template>

                    {% if settings.enable_alert_msg %}
                      {% if customer.tags contains 'cj-import' %}
                        {% if customer.tags contains 'cj_paypal' or customer.tags contains 'cj-paypal' %}
                        {% if customer.tags contains 'subr-status-active' or customer.tags contains 'subr-status-mixed' or customer.tags contains 'token_missing' %}
                          <div class="subcription-infobar d-flex align-items-center px-3 px-md-5 py-4 my-5">
                            <img src="https://cdn.shopify.com/s/files/1/1091/8372/files/infobar.svg?92414" width="22" class="mr-3 mr-md-5" alt="infobar-icon" style="min-width:22px;">
                            <div class="subcription-infobar-text">
                              {% if settings.alert_head_txt != blank %}<h6 class="font-size-sm mb-0">{{ settings.alert_head_txt }}</h6>{% endif %}
                              {% if settings.alert_caption_txt != blank %}<p class="mb-0 font-size-sm">{{settings.alert_caption_txt }}{% if settings.alert_link_txt %}<a href="{{settings.alert_link_url}}" class="font-barlow font-size-sm font-weight-medium text-uppercase ls-1 text-primary ml-2 d-inline-block align-middle">{{settings.alert_link_txt }}<i class="ion ion-ios-arrow-forward ml-2"></i></a>{% endif %}</p>{% endif %}
                            </div>
                          </div>
                          {% endif %}
                        {% endif %}
                      {% endif %}
                    {% endif %}

                     <div class="account-subcription-header mb-3 pt-lg-3">
                       <h5 class="mb-0">Order History</h5>
                     </div>

                     {% paginate customer.orders by 15 %}
                     {% if customer.orders_count > 0 %}
                      <div class="subscription-order-table order-history">
                        <div class="subscription-header bg-secondary py-2">
                          <div class="px-3">
                            <div class="row align-items-center">
                              <div class="col-md-6 pl-md-0">
                                <p class="mb-0 font-barlow font-size-sm font-weight-medium text-white text-uppercase">Order & Date</p>
                              </div>
                              <div class="col-md-3 d-none d-md-block">
                                <p class="mb-0 font-barlow font-size-sm font-weight-medium text-white text-uppercase">Payment Status</p>
                              </div>
                              <div class="col-md-3 text-right d-none d-md-block">
                                <p class="mb-0 font-barlow font-size-sm font-weight-medium text-white text-uppercase">total</p>
                              </div>
                            </div>
                          </div>
                        </div>
                        <div class="subscription-body border border-secondary">
                          {% for order in customer.orders %}
                          <div class="subscription-item px-md-3 {% if forloop.last != true %}border-bottom border-secondary{% endif %} accordion" id="accordionThird_{{order.id}}">
                            <div class="row align-items-center">
                              <div class="col-md-6 pl-md-0">
                                <div class="d-flex align-items-center">
                                  <div class="d-md-none mr-3" id="headingThird_{{order.id}}">
                                    <button class="btn btn-link px-0 py-2 border-0 btn-collapseProduct collapsed" type="button" data-toggle="collapse" data-target="#collapse_{{order.id}}" aria-expanded="false" aria-controls="collapse_{{order.id}}">
                                      <i class="ion ion-ios-add h5 mb-0"></i>
                                      <i class="ion ion-ios-remove h5 mb-0"></i>
                                    </button>
                                  </div>
                                  <p class="mb-0"><a href="{{order.customer_url}}" class="text-primary font-weight-semi-bold">{{ order.name }}</a> on {{ order.created_at | date: "%B %d, %Y" }}</p>
                                </div>
                              </div>
                              <div class="col-md-3 d-none d-md-block">
                                <p class="mb-0">{{ order.financial_status_label }}</p>
                              </div>
                              <div class="col-md-3 text-right d-none d-md-block">
                                <p class="mb-0">{{ order.total_price | money }}</p>
                              </div>
                              <div id="collapse_{{order.id}}" class="collapse col-12 d-md-none" aria-labelledby="headingThird_{{order.id}}" data-parent="#accordionThird_{{order.id}}">
                                <div class="border-top border-secondary mt-3 pt-3">
                                  <div class="row mb-2">
                                    <div class="col-6"><p class="mb-0 font-barlow font-weight-medium text-uppercase">Payment Status</p></div>
                                    <div class="col-6"><p class="mb-0">{{ order.financial_status_label }}</p></div>
                                  </div>
                                  <div class="row mb-0">
                                    <div class="col-6"><p class="mb-0 font-barlow font-weight-medium text-uppercase">total</p></div>
                                    <div class="col-6"><p class="mb-0">{{ order.total_price | money }}</p></div>
                                  </div>
                                </div>
                              </div>
                            </div>
                          </div>
                          {% endfor %}
                        </div>
                      </div>

                      {% if paginate.pages > 1 %}
                        <div class="text-center mt-3">
                        {% include 'pagination' %}
                        </div>
                      {% endif %}
                     {% else %}
                      <div class="pt-lg-3">
                        <p class="mb-0 ls-1">You haven't placed any orders yet.</p>
                      </div>
                     {% endif %}
                    {% endpaginate %}
                   </div>
                 </div>
               </div>
             </div>
           </div>

           <div class="tab-pane" id="subscriptions" role="tabpanel" aria-labelledby="subscriptions-tab">
             <div class="row justify-content-center mb-3">
               <div class="col-12 heading-wrapper">
                 <h2 class=" text-center mb-4">My Subscriptions</h2> 

                {% if settings.enable_alert_msg %}
                  {% if customer.tags contains 'cj-import' %}
                    {% if customer.tags contains 'cj_paypal' or customer.tags contains 'cj-paypal' %}
                    {% if customer.tags contains 'subr-status-active' or customer.tags contains 'subr-status-mixed' or customer.tags contains 'token_missing' %}
                      <div class="subcription-infobar d-flex align-items-center px-3 px-md-5 py-4">
                        <img src="https://cdn.shopify.com/s/files/1/1091/8372/files/infobar-icon.png?21120" width="22" alt="" class="mr-3 mr-md-5" style="min-width:22px;"> 
                        <div class="subcription-infobar-text">
                          {% if settings.alert_head_txt != blank %}<h6 class="font-size-sm mb-0">{{ settings.alert_head_txt }}</h6>{% endif %}
                          {% if settings.alert_caption_txt != blank %}<p class="mb-0 font-size-sm">{{settings.alert_caption_txt }}{% if settings.alert_link_txt %}<a href="{{settings.alert_link_url}}" class="font-barlow font-size-sm font-weight-medium text-uppercase ls-1 text-primary ml-2 d-inline-block align-middle">{{settings.alert_link_txt }}<i class="ion ion-ios-arrow-forward ml-2"></i></a>{% endif %}</p>{% endif %}                    
                        </div>
                      </div>
                      {% endif %}
                    {% endif %}
                  {% endif %}
                {% endif %}
               </div>
             </div>

              <template v-if="rechargeDashboard.subscriptions.length > 0">
               <div class="row justify-content-center">
                <div class="col-12 my-3 {% if settings.enable_productaddon %}col-xl-8{% endif %}">

                   <template v-if="rechargeDashboard.activeSubscription.length > 0">
                     <div class="shadow px-3 px-md-4 pt-4 pt-md-6 pb-0 change-subscription mb-custom">
                       <div class="d-sm-flex flex-wrap align-items-center justify-content-between mb-3 text-center">
                         <h5 class="mb-2 mb-sm-0">Subscription(s):</h5>
                         {% comment %} <template v-if="!rechargeDashboard.subscriptions.length == 0">  
                           <a href="#" class="line-hover-gray font-size-ex-sm">Don�t See Your Subscription?</a>
