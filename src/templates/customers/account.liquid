                     <div class="col-md-6 my-3">
                       <input type="text" placeholder="Name" name="contact[name]" value="{% if form[name] %}{{ form[name] }}{% elsif customer %}{{ customer.name }}{% endif %}" spellcheck="false" autocomplete="off" autocapitalize="off" required class="form-control font-base ls-base">
                     </div>
                     <div class="col-md-6 my-3">
                       <input type="email" placeholder="Email Address" name="contact[email]" value="{% if form.email %}{{ form.email }}{% elsif customer %}{{ customer.email }}{% endif %}" class="form-control font-base ls-base" spellcheck="false" autocomplete="off" autocapitalize="off" required>
                     </div>
                     <div class="col-md-12 my-3">
                       <select class="form-control font-base ls-base"  name="contact[subscription]">
