      {% if block.settings.past_boxes %}
        <div class="section-content past-boxes-section">
          <div class="{% if section.settings.fluid_layout %}container-fluid{% else %}container{% endif %}">
            {% if block.settings.heading != blank %}
              <div class="row">
                <div class="col-12 text-center">
                  <h2 class="section-title mb-0">{{ block.settings.heading }}</h2>
                </div>
              </div>
            {% endif %}
            {% assign blog = blogs[block.settings.blog] %}
            <div class="carousel row" data-past-box role="tree" data-flickity='{
                            "contain": true,
                            "wrapAround": true,
                            "accessibility": true,
                            "setGallerySize": true,
                            "cellAlign": "left",
                            "groupCells": "100%",
                            "lazyLoad": false,
                            "prevNextButtons": true,
                            "pageDots": false,
                            "arrowShape": {"x0": 10, "x1": 60, "y1": 50, "x2": 65, "y2": 40, "x3": 25}
                        }'>

              {% for article in blog.articles %}
                <div class="carousel-cell d-flex equal-height col-12 col-sm-6 col-md-4 col-lg-3 py-5" role="treeitem">
                  {% assign producturl = '' %}
                  {% assign producturl_article =  '' %}
                  {% assign pastProd = '' %}
                  {% for tag in article.tags %}
                    {% if tag contains 'product_' %}
                      {% assign producturl = tag | split: '_' %}
                      {% assign producturl_article =  producturl[1] %}
                    {% endif %}
                  {% endfor %}
                  {% if producturl_article.size > 0 %}
                    {% assign pastProd = all_products[producturl_article] %}
                  {% endif %}
                  <div class="card shadow border-0 p-3">
                    <div class="card-img">
                      <a href="{% if pastProd.size > 0 %}{{pastProd.url}}{% else %}{{article.url}}{% endif %}" class="position-relative product-img">
                        <img
                                data-sizes="auto"
                                src="{{ article.image.src | img_url: 'pico'}}"
                                data-srcset="
                                                    {{ article.image.src | img_url: '800x500', crop: 'center' }} 576w
                                                    "
                                class="lazyload img-fluid h-100"
                                alt="{{ article.image.alt | escape }}"/>
                      </a>
                    </div>
                    <div class="card-body px-0 pb-2 text-center">
                      <p class="text-uppercase font-size-sm mb-1"><a href="{% if pastProd.size > 0 %}{{pastProd.url}}{% else %}{{article.url}}{% endif %}">{{ article.published_at | date: "%b %Y" }}: {{ article.title }}</a></p>
                      {{ article.excerpt }}
                    </div>
                  </div>
                </div>
              {% endfor %}
            </div>
            <div class="row mt-4">
              <div class="col-12 text-center">
                {% if block.settings.btn_txt != '' %}
                  <a href="{{ blog.url }}" class="btn btn{% if block.settings.btn_type == 'outline' %}-{% endif %}{{ block.settings.btn_type }}-{{ block.settings.btn_variant }} {{ block.settings.btn_size }} mr-3">{{ block.settings.btn_txt }}</a>
