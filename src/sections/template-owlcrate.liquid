<section data-section-id="{{ section.id }}" data-template="box-subscription">
  {% for block in section.blocks %}
    {% if block.type == 'page_banner' %}
      {% if block.settings.page_banner %}
        <div class="page-banner text-center text-md-left">
          <img
                  data-sizes="auto"
                  src="{{ block.settings.image | img_url: 'small'}}"
                  data-srcset="
                        {{ block.settings.image | img_url: '1440x' }} 992w,
                        {{ block.settings.image | img_url: '1200x' }} 1200w
                        "
                  class="lazyload h-100 d-none d-md-block banner-img" alt="{{ product.title | escape }}" />
          {% if block.settings.mobile_image == blank %}
            <img
                    data-sizes="auto"
                    src="{{ block.settings.image | img_url: 'small'}}"
                    data-srcset="
                        {{ block.settings.image | img_url: '576x' }} 576w,
                        {{ block.settings.image | img_url: '768x' }} 768w
                        "
                    class="lazyload h-100 d-block d-md-none banner-img" alt="{{ product.title | escape }}" />
          {% else %}
            <img
                    data-sizes="auto"
                    src="{{ block.settings.mobile_image | img_url: 'small'}}"
                    data-srcset="
                            {{ block.settings.mobile_image | img_url: '576x' }} 576w,
                            {{ block.settings.mobile_image | img_url: '768x' }} 768w
                            "
                    class="lazyload h-100 d-md-none banner-img" alt="{{ product.title | escape }}" />
          {% endif %}
          <div class="cation py-3 py-md-0">
            <div class="{% if section.settings.fluid_layout %}container-fluid{% else %}container{% endif %}">
              <div class="row no-gutters align-items-center">
                <div class="col-md-6  mb-md-0 mb-5 pr-ex-lg-10 text-md-left text-center">
                  {% if block.settings.title != '' %}<h1 class="text-white mb-0">{{ block.settings.title }}</h1>{% endif %}
                  {% if block.settings.caption != blank %}<p class="lead text-white mb-3 mb-md-4">{{ block.settings.caption }}</p>{% endif %}
                  {% if block.settings.heading_box != blank or block.settings.link_text %}
                    <div class="d-md-flex align-items-center mb-4 mb-md-5">
                      {% if block.settings.image_box  %}<img
                        data-sizes="auto"
                        src="{{ block.settings.image_box | img_url: 'small'}}"
                        data-srcset="
                                            {{ block.settings.image_box | img_url: '450x' }} 576w,
                                            {{ block.settings.image_box | img_url: '400x' }} 992w,
                                            {{ block.settings.image_box | img_url: '450x' }} 1200w
                                            "
                        class="lazyload sub-box-img-shadow mb-3 mb-md-0 mr-md-5 box-img" alt="{{ block.settings.heading_box | escape }}" />{% endif %}
                      <div class="box-text">
                        <h6 class="font-size-sm text-white mb-1">{{ block.settings.heading_box }}</h6>
                        <h5 class="text-white ls-base mb-0">{{ block.settings.caption_box | escape }}</h5>
                        {%- if block.settings.link_text != blank and block.settings.theme_link != blank and block.settings.enabled == true -%}
                          <a href="{{block.settings.theme_link}}" class="theme-link font-barlow mb-0 pt-md-4" style="color: {{block.settings.link_color | default: '#fff' }} !important;">
                            {{ block.settings.link_text }}</a>
                        {%- endif -%}
                      </div>
                    </div>
                  {% endif %}
                  {% if block.settings.enable_timer and block.settings.choose_date != blank %}
                    {% assign now =  "now" | date: "%s" %}
                    {% assign choose_date = block.settings.choose_date | date: "%s" %}
                    {% if choose_date > now %}
                      <h6 class="font-sie-sm text-white mb-3"><img src="https://cdn.shopify.com/s/files/1/1091/8372/files/clock-icon.svg?38760" alt="clock Icon" class="mr-3" style="Width:1.8125rem;">{{ block.settings.sale_text | escape }}</h6>
                      <div class="mb-5 mb-md-6">
                        <template>
                          <Countdown end="{{block.settings.choose_date}}"></Countdown>
                        </template>
                      </div>
                    {% endif %}
                  {% endif %}
                  <div class="d-flex align-items-center justify-content-center justify-content-md-start">
                    <template v-if="isUtmAdult">
                      {% if block.settings.btn_txt != '' %}
                        <a href="/pages/subscription-plan"  class="btn btn{% if block.settings.btn_type == 'outline' %}-{% endif %}{{ block.settings.btn_type }}-{{ block.settings.btn_variant }} {{ block.settings.btn_size }} mr-3 mr-lg-4">{{ block.settings.btn_txt }}</a>
                      {% endif %}
                      {% if block.settings.btn_txt_second != '' %}
                        <a href="#" @click.prevent="_chooseGiftProductOnPage('type-gift')" class="btn btn{% if block.settings.btn_type_second == 'outline' %}-{% endif %}{{ block.settings.btn_type_second }}-{{ block.settings.btn_variant_second }} {{ block.settings.btn_size_second }}">{{ block.settings.btn_txt_second }}</a>
                      {% endif %}
                    </template>
                    <template v-else>
                      {% if settings.adult_soldout %}
                        <a href="#" @click.prevent="_soldoutProduct('plan_adult', 'type-normal')" class="btn btn{% if block.settings.btn_type == 'outline' %}-{% endif %}{{ block.settings.btn_type }}-{{ block.settings.btn_variant }} {{ block.settings.btn_size }} mr-3 mr-lg-4">{{ settings.modal_adult_btn_txt }}</a>
                      {% else %}
                        {% if block.settings.btn_txt != '' %}
                          <a href="/pages/subscription-plan"  class="btn btn{% if block.settings.btn_type == 'outline' %}-{% endif %}{{ block.settings.btn_type }}-{{ block.settings.btn_variant }} {{ block.settings.btn_size }} mr-3 mr-lg-4">{{ block.settings.btn_txt }}</a>
                        {% endif %}
                        {% if block.settings.btn_txt_second != '' %}
                          <a href="#" @click.prevent="_chooseGiftProductOnPage('type-gift')" class="btn btn{% if block.settings.btn_type_second == 'outline' %}-{% endif %}{{ block.settings.btn_type_second }}-{{ block.settings.btn_variant_second }} {{ block.settings.btn_size_second }}">{{ block.settings.btn_txt_second }}</a>
                        {% endif %}
                      {% endif %}
                    </template>
                  </div>
                  <div class="banner-text-caption-home-page d-md-block d-none">
                    {% if block.settings.banner_text_checkbox %}
                      <p class="text-white font-barlow mb-0 pt-md-4" style="font-size:{{ block.settings.banner_test_font_size}}px ">{{ block.settings.banner_text }} </p>
                    {% endif %}
                  </div>
                </div>
                <div class="col-md-5 d-md-none ml-auto"><img src="{{block.settings.product_image | img_url: 'pico'}}" data-src="{{block.settings.product_image | img_url: 'large'}}" alt="Your Favorite Monthly Book Subscription Box!" class="w-100 lazyload"></div>
              </div>
            </div>
          </div>
          <div class="banner-text-caption text-center d-md-none d-block  position-relative">
            {% if block.settings.banner_text_checkbox %}
              <p class="text-white mb-0 px-4 pb-4" style="font-size:{{ block.settings.banner_test_font_size }}px ">{{ block.settings.banner_text }} </p>
            {% endif %}
          </div>
        </div>
      {% endif %}
    {% endif %}
  {% endfor %}

  {% if section.settings.enable_featuredsection %}
    <div class="index-features-grid section-content mt-4 mt-md-7">
      <div class="{% if section.settings.fluid_layout %}container-fluid{% else %}container{% endif %}">
        {% if section.settings.feature_heading != blank %}
          <div class="row">
            <div class="col-12 text-center">
              <h2 class="section-title mb-3 mb-md-5">{{ section.settings.feature_heading }}</h2>
            </div>
          </div>
        {% endif %}
        <div class="row">
          {% for block in section.blocks %}
            {% if block.type == 'feature' %}
              {% if block.settings.feature_block %}
                <div class="{% unless section.settings.grid_size == '12' %}col-sm-12{% endunless %} col-md-{{ section.settings.grid_size }}">
                  <div class="feature my-3">
                    <div class="card-img text-center d-flex justify-content-center align-items-end mb-3 mb-md-4">
                      {% if block.settings.icon_class != blank %}
                        <img src="{{block.settings.icon_class}}" alt="{{ block.settings.heading_feature | escape }}" class="mh-100 mw-10" />
                      {% elsif block.settings.image_feature != blank %}
                        <img src="{{block.settings.image_feature | img_url :'165x' }}" alt="{{ block.settings.heading_feature | escape }}" class="mh-100 mw-10" />
                      {% endif %}
                    </div>
                    <div class="text-center px-lg-3">
                      {% if block.settings.heading_feature != blank %}<h5 class="mb-1 mb-md-2">{{ block.settings.heading_feature }}</h5>{% endif %}
                      {% if block.settings.txt != blank %}<div class="feature-details">{{ block.settings.txt }}</div>{% endif %}
                    </div>
                  </div>
                </div>
              {% endif %}
            {% endif %}
          {% endfor %}
        </div>
      </div>
    </div>
  {% endif %}

  {% if section.settings.enable_newsletter %}
    <div class="owlcrate-jr-newsletter section-content">
      <div class="container">
        <div class="" style="background-color: #f5f5f5;">
          <div class="row align-items-center">
            <div class="col-ex-lg-11 mx-auto">
              <div class=" jr-newsletter py-md-5 py-6" style="background-image: url('https://cdn.shopify.com/s/files/1/1091/8372/files/OwlcrateJr_Product_Update_Owlcrate.png?v=1597125921');">
                <div class="row">
                  <div class="col-lg-10">
                    <div class="d-md-flex align-items-center justify-content-lg-start justify-content-between text-md-left text-center px-md-3 px-2 px-ex-lg-0 px-lg-3">
                      <div class="newsletter-content mr-lg-7 mr-md-5 mb-md-0 mb-5">
                        <h5 class="font-barlow font-weight-semi-bold mb-0 text-uppercase font-size-lg">Subscribe to the</h5>
                        <h4 class="text-capitalize mb-0 font-weight-bold">OwlCrate newsletter</h4>
                      </div>
                      <div class="newsletter-div">
                        <form data-klaviyo-form-jr id="klaviyo_form_jr" class="validate contact-form klaviyo_gdpr_embed_{{section.settings.klaviyo_jr}}"  action="//manage.kmail-lists.com/subscriptions/subscribe" data-ajax-submit="//manage.kmail-lists.com/ajax/subscriptions/subscribe" method="GET" target="_blank" novalidate="novalidate">
                          <input type="hidden" name="g" value="{{section.settings.klaviyo_jr}}" data-input-hidden>
                          <div>
                            <div class=" input-group input-group-sm">
                              <input type="email" value="" name="email" class="form-control font-base ls-base" id="k_id_email" placeholder="{{ 'general.newsletter_form.newsletter_email' | t }}" aria-label="email">
                              <div class="input-group-append"><input type="submit" value="SUBMIT" name="subscribe" class="btn btn-dark font-size-ex-sm px-lg-3 py-2"></div>
                            </div>
                            <div class="klaviyo_messages mt-3 text-center">
                              <div class="success_message text-success" style="display:none;" data-success_message></div>
                              <div class="error_message  text-danger" style="display:none;" data-error_message></div>
                            </div>
                          </div>
                        </form>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  {% endif %}

  {% if section.settings.enable_plan %}
    <div class="section-content index-owlCrate-plan">
      <div class="{% if section.settings.fluid_layout %}container-fluid{% else %}container{% endif %}">
        <div class="row justify-content-center mb-4">
          <div class="col-12 col-md-7 text-center">
            {% if section.settings.plan_heading %}<h2 class="mb-1 section-title">{{section.settings.plan_heading}}</h2>{% endif %}
            {% if section.settings.plan_caption %}<p>{{section.settings.plan_caption}}</p>{% endif %}
          </div>
        </div>
        <div class="row justify-content-center" v-if="subscriptionFlow.subscriptionPlanJson.length > 0">
          <div class="col-12 col-lg-9">
            <div class="row align-items-end">
              <div class="col-md-4 mb-3 mb-md-0" v-for="variant in subscriptionFlow.subscriptionPlanJson"  v-if="variant.is_hidden != 'true'" >
                <div class="card shadow-none border-secondary plan-item">
                  <p v-if="variant.save > 0" class="bg-blurple-100 text-center py-1 mb-0 text-white ls-1 font-weight-semi-bold font-size-xs offer-text" v-text="'You Save '+ _formatMoney(variant.save)+'!'"></p>
                  <div class="card-body d-flex align-items-center d-md-block py-5">
                    <div class="d-md-none plan-mobile-img">
                      <div class="product-img">
                        <img test v-if="variant.featured_image != null" :src="_getSizedImageUrl(variant.featured_image, 'pico')"
                             :data-src="_getSizedImageUrl(variant.featured_image, '480x')"
                             class="img-fluid h-100 lazyload"
                             :alt="variant.title"/>
                        <img v-else-if="variant.title.includes('3')" src="{{'3-month-plan.jpg' | file_img_url: 'pico'}}" data-src="{{'3-month-plan.jpg' | file_img_url: '480x'}}" :alt="variant.title" class="img-fluid h-100 lazyload" />
                        <img v-else-if="variant.title.includes('6')" src="{{'6-month-plans.jpg' | file_img_url: 'pico'}}" data-src="{{'6-month-plans.jpg' | file_img_url: '480x'}}" :alt="variant.title" class="img-fluid h-100 lazyload" />
                        <img v-else src="{{'owl-pack.jpg' | file_img_url: 'pico'}}" data-src="{{'owl-pack.jpg' | file_img_url: '480x'}}" :alt="variant.title" class="img-fluid h-100 lazyload" />
                      </div>
                    </div>
                    <div class="plan-item-text text-left text-md-center">
                      <h6 class="mb-1" v-text="variant.title"></h6>
                      <h5 v-text="_formatMoney(variant.price)"></h5>
                      <div class="product-img mb-4 d-none d-md-block">
                        <img v-if="variant.featured_image != null" :src="_getSizedImageUrl(variant.featured_image, 'pico')"
                             :data-src="_getSizedImageUrl(variant.featured_image, '480x')"
                             class="img-fluid h-100 lazyload"
                             :alt="variant.title"/>
                        <img v-else-if="variant.title.includes('3')" src="{{'3-month-plan.jpg' | file_img_url: 'pico'}}" data-src="{{'3-month-plan.jpg' | file_img_url: '480x'}}" class="img-fluid h-100 lazyload" />
                        <img v-else-if="variant.title.includes('6')" src="{{'6-month-plans.jpg' | file_img_url: 'pico'}}" data-src="{{'6-month-plans.jpg' | file_img_url: '480x'}}" class="img-fluid h-100 lazyload" />
                        <img v-else src="{{'owl-pack.jpg' | file_img_url: 'pico'}}" data-src="{{'owl-pack.jpg' | file_img_url: '480x'}}" class="img-fluid h-100 lazyload" />
                      </div>
                      <p class="mb-0 card-total font-size-ex-sm" v-match-heights="{el: ['.card-total']}"><b>Total Price:</b> <br class="d-md-none" /> <span v-text="_formatMoney(variant.price) + ' + Shipping'"></span></p>
                      <template v-if="variant.metafields.charge_internal_frequency != null">
                        <p class="mb-0 font-size-ex-sm" v-if="variant.metafields.charge_internal_frequency == 1" >One-Time Box</p>
                        <p class="mb-0 font-size-ex-sm" v-else v-text="'One-Time '+variant.metafields.charge_internal_frequency+'-Month Subscription'"></p>
                      </template>
                      <template v-if="isUtmAdult">
                        <a href="#"  @click.prevent="_selectedPagePlan(variant)" class="btn btn-sm btn-dark mt-2 mt-md-3">Select</a>
                      </template>
                      <template v-else>
                        {% if settings.adult_soldout == false %}
                          <a href="#"  @click.prevent="_selectedPagePlan(variant)" class="btn btn-sm btn-dark mt-2 mt-md-3">Select</a>
                        {% endif %}
                      </template>
                    </div>
                  </div>
                </div>
              </div>
              {% if settings.adult_soldout %}
                <div class="col-12 text-center" v-show="isUtmAdult == false">
                  <a href="#" @click.prevent="_soldoutProduct('plan_adult', 'type-normal')"  class="btn btn-sm btn-dark mt-2 mt-md-3">{{ settings.modal_adult_btn_txt }}</a>
                </div>
              {% endif %}
            </div>
          </div>
        </div>
      </div>
    </div>
  {% endif %}

  {% for block in section.blocks %}
    {% if block.type == 'feature_owlcrate' %}
      {% if block.settings.feature_owlcrate %}
        <div class="section-content feature-owlcrate py-7 py-md-10 my-3 my-md-6" style="background-color: {{ block.settings.section_bg_color }};">
          <div class="{% if section.settings.fluid_layout %}container-fluid{% else %}container{% endif %}">
            {% if block.settings.heading_owlcrate != blank %}
              <div class="row justify-content-center mb-3 mb-md-7">
                <div class="col-12 col-md-7 text-center">
                  <h2 class="mb-1 section-title">{{ block.settings.heading_owlcrate }}</h2>
                </div>
              </div>
            {% endif %}
            <div class="row align-items-center no-gutters justify-content-center">
              <div class="col-12 col-md-6 pr-md-4 mb-4 d-ex-lg-none">
                <div class="position-relative product-img owlcrate-images">
                  <img
                          data-sizes="auto"
                          src="{{ block.settings.image_owlcrate | img_url: 'small'}}"
                          data-srcset="
                                        {{ block.settings.image_owlcrate | img_url: '450x' }} 576w
                                        "
                          class="lazyload w-auto h-100" alt="{{ block.settings.heading_owlcrate | escape }}" />
                </div>
              </div>
              <div class="col-12 col-md-6 col-ex-lg-12">
                <div class="row no-gutters justify-content-center">
                  <div class="col-md-6 col-ex-lg-6 col-xl-5 order-ex-lg-2 mb-6 mb-ex-lg-0 d-none d-ex-lg-block">
                    <div class="position-relative product-img owlcrate-images">
                      <img
                              data-sizes="auto"
                              src="{{ block.settings.image_owlcrate | img_url: 'small'}}"
                              data-srcset="
                                                {{ block.settings.image_owlcrate | img_url: '450x' }} 992w
                                                "
                              class="lazyload w-auto h-100" alt="{{ block.settings.heading_owlcrate | escape }}" />
                    </div>
                  </div>
                  <div class="col-ex-lg-3 order-ex-lg-1">
                    {% if block.settings.features_item_fist != '' %}
                      <div class="features-item fist-item">
                        <p class="mb-0 lead">{{ block.settings.features_item_fist }}</p>
                        <span class="box-line"><span class="box-circle"></span></span>
                      </div>
                    {% endif %}
                    {% if block.settings.features_item_second != '' %}
                      <div class="features-item second-item">
                        <p class="mb-0 lead">{{ block.settings.features_item_second }}</p>
                        <span class="box-line"><span class="box-circle"></span></span>
                      </div>
                    {% endif %}
                  </div>
                  <div class="col-ex-lg-3 order-ex-lg-3">
                    {% if block.settings.features_item_third != '' %}
                      <div class="features-item third-item pr-xl-4">
                        <p class="mb-0 lead">{{ block.settings.features_item_third }}</p>
                        <span class="box-line"><span class="box-circle"></span></span>
                      </div>
                    {% endif %}
                    {% if block.settings.features_item_four != '' %}
                      <div class="features-item four-item pr-xl-5">
                        <p class="mb-0 lead">{{ block.settings.features_item_four }}</p>
                        <span class="box-line"><span class="box-circle"></span></span>
                      </div>
                    {% endif %}
                  </div>
                </div>
              </div>
            </div>
            {% if block.settings.btn_txt != '' %}
              <div class="row justify-content-center align-items-center mt-3">
                <div class="col-12 text-center">
                  <template v-if="isUtmAdult">
                    <a href="/pages/subscription-plan" class="btn btn{% if block.settings.btn_type == 'outline' %}-{% endif %}{{ block.settings.btn_type }}-{{ block.settings.btn_variant }} {{ block.settings.btn_size }}">{{ block.settings.btn_txt }}</a>
                  </template>
                  <template v-else>
                    <a {% if settings.adult_soldout %} href="#" @click.prevent="_soldoutProduct('plan_adult', 'type-normal')" {% else %} href="/pages/subscription-plan"{% endif %} class="btn btn{% if block.settings.btn_type == 'outline' %}-{% endif %}{{ block.settings.btn_type }}-{{ block.settings.btn_variant }} {{ block.settings.btn_size }}">{% if settings.adult_soldout %} {{ settings.modal_adult_btn_txt }} {% else %} {{ block.settings.btn_txt }}{% endif %}</a>
                  </template>
                </div>
              </div>
            {% endif %}
          </div>
        </div>
      {% endif %}
    {% endif %}
  {% endfor %}

  {%- if  section.settings.enable_faq -%}
    <div class="section-content gift-faq">
      <div class="{% if section.settings.fluid_layout_faq %}container-fluid{% else %}container{% endif %}">
        {% if section.settings.faq_heading != '' %}
          <div class="text-center">
            <h2 class="mb-4">{{ section.settings.faq_heading }}</h2>
          </div>
        {% endif %}
        <div class="row justify-content-center">
          <div class="col-12 px-xl-7">
            <div class="accordion" id="faq">
              {% for block in section.blocks %}
                {% if block.type == 'faq' %}
                  {% if block.settings.faq_check and block.settings.heading != '' %}
                    <div class="border-top border-secondary faq-item">
                      <div class="py-3 px-4" id="heading{{ forloop.index }}">
                        <h6 class="m-0 text-capitalize ls-base">
                          <div class="d-flex align-items-center justify-content-between w-100 collapsed" data-toggle="collapse" data-target="#collapse{{ forloop.index }}" aria-expanded="false" aria-controls="collapseOne">
                            <span class="heading">{{ block.settings.heading }}</span>
                            <i class="ion ion-ios-remove h5 mb-0 ml-3"></i>
                          </div>
                        </h6>
                      </div>

                      {% if block.settings.txt != '' %}
                        <div id="collapse{{ forloop.index }}" class="collapse" aria-labelledby="heading{{ forloop.index }}" data-parent="#faq">
                          <div class="card-body px-4 pt-1 pb-5">
                            {{ block.settings.txt }}
                          </div>
                        </div>
                      {% endif %}
                    </div>
                  {% endif %}
                {% endif %}
              {% endfor %}
            </div>
          </div>
        </div>
      </div>
    </div>
  {%- endif -%}

  {% if section.settings.enabled_yotpo_reviews %}
    {% include 'reviews-yotpo' %}
  {% endif %}


  {% if section.settings.enable_featuredyoutube %}
    <div class="section-content featured-youtube">
      <div class="{% if section.settings.fluid_layout %}container-fluid{% else %}container{% endif %}">
        {% if section.settings.feature_youtube_heading != blank %}
          <div class="row">
            <div class="col-12 text-center">
              <h2 class="section-title mb-1">{{ section.settings.feature_youtube_heading }}</h2>
            </div>
          </div>
        {% endif %}
        <div class="carousel row px-lg-5" role="tree"
             data-flickity='{
                    "contain": true,
                    "wrapAround": false,
                    "freeScroll": false,
                    "groupCells": "100%",
                    "dragThreshold": 20,
                    "lazyLoad": false,
                    "cellAlign": "center",
                    "prevNextButtons": true,
                    "pageDots": false,
                    "arrowShape": {"x0": 10, "x1": 60, "y1": 50, "x2": 65, "y2": 40, "x3": 25}
                    }'>
          {% for block in section.blocks %}
            {% if block.type == 'feature_youtube' %}
              <div class="carousel-cell col-11 col-md-8 col-lg-6 my-5 px-lg-4" role="treeitem">
                <div class="bg-white shadow">
                  {% comment %} <video id="video-section-player_{{forloop.index}}" class="video-js vjs-default-skin vjs-big-play-centered vjs-fluid vjs-16-9"
                     controls
                     preload="auto"
                     poster="{{ block.settings.poster_image | img_url: '500x' }}"
                     data-setup='{
                     "techOrder": ["youtube"],
                     "sources": [
                         { "type": "video/youtube", "src": "https://www.youtube.com/watch?v={{ block.settings.video_url }}"}
                         ]
                     }'>
                 </video> {% endcomment %}
