}

.insta-slider .insta-slide {
  width: 33.4027778%;
  padding: 1.94444444%;
}

.insta-slider .insta-slide.is-selected .insta-image-wrapper {
  width: 100%;
  margin: 0;
}

.insta-slider .insta-slide.is-next .insta-image-wrapper {
  margin-left: 0;
}

.insta-slider .insta-slide:not(.is-selected):not(.is-next):not(.is-prev) {
  opacity: 0;
  transition-delay: .5s;
}

.insta-image-wrapper {
  width: 64.7058824%;
  margin: 17.6470588%;
  margin-left: 35.2941176%;
  transition: all .5s ease;
  position: relative;
}

.insta-image-wrapper:before {
  content: '';
  display: block;
  padding-bottom: 100%;
}

.insta-image-wrapper img {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  object-fit: cover;
  border-radius: 18px;
}

.insta-slider .flickity-button {
  width: 70px;
  height: 70px;
  background: transparent url({{ 'next.png' | asset_img_url: '200x' }}) no-repeat center/contain;
  opacity: 1;
}

.insta-slider .flickity-button:focus {
  outline: none;
  box-shadow: none;
}

.insta-slider .flickity-button.previous {
  background: url({{ 'prev.png' | asset_img_url: '200x' }}) no-repeat center/contain;
  left: 5%;
  margin-left: -50px;
}

.flickity-prev-next-button.next {
  right: 5%;
  margin-right: -50px;
}

.insta-slider .flickity-button svg {
  display: none;
}

