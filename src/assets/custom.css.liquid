}

.accordion {
  width: 100%;
}

.accordion .accordion-item:first-child {
  border-top: 1px solid rgba(0, 0, 0, 0.5);
}

.accordion-item {
/*   background: #FFFFFF; */
  /* box-shadow: 0 6px 20px #0000004d; */
  /* border-radius: 10px; */
  /* margin-bottom: 30px; */
  padding: 20px;
  transition: margin .3s ease;
  border-bottom: 1px solid rgba(0, 0, 0, 0.5);
  cursor: pointer;
}

.accordion-item.accordion-item-active {
/*   margin-bottom: 74px; */
  transition: margin 0.3s ease;
}

.accordion-btn {
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  /*     padding: 27px 14px; */
  text-align: left;
  cursor: pointer;
  transition: 0.1s;
  color: #000000;
  border: none;
  outline: none;
  background: #ffffff;
  border-radius: 10px;

  font-family: 'Muli';
  font-size: 20px;
  font-style: normal;
  font-weight: 400;
  line-height: 26px;
  letter-spacing: 0.005em;
  text-align: left;

}

.arrow-right {
  width: 24px;
  height: 24px;
  display: flex;
}

.accordion-item-active .accordion-btn {
  color: #000;
}

.accordion-icon {
  border-radius: 50%;
  transform: rotate(0deg);
  transition: 0.3s ease-in-out;
  opacity: 0.9;
  margin-left: 14px;
  vertical-align: top;
  display: flex;
}


.accordion-item-active .accordion-icon {
  transform: rotate(224deg);
}

.accordion-content {
  font-weight: 300;
  max-height: 0;
  opacity: 0;
  overflow: hidden;
  color: #000;
  transform: translateX(16px);
  transition: max-height 0.5s ease, opacity 0.5s, transform 0.5s;
}

.accordion-content p,
.accordion-content .accordion-content-data {
  padding: 6px 27px;
  margin-top: 12px;
  margin-bottom: 0;
  border-left: 5px solid rgba(0,0,0,.5);
  font-size: 18px;
  font-style: normal;
  font-weight: 300;
  line-height: 20px;
  letter-spacing: .005em;
  text-align: left;
}

.accordion-item-active .accordion-content {
  transition: max-height 0.5s ease, opacity 0.5s, transform 0.5s;
}

.accordion-item-active .accordion-content {
  opacity: 1;
  transform: translateX(0px);
  max-height: 100vh;
}

.accordian-plus-minus {
  width: 40px;
