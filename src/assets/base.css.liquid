}

.text-center {
  text-align: center;
}

.overlay {
  position: absolute;
  top: 0px;
  left: 0%;
  width: 100%;
  height: 100%;
  background-color: transparent;
  -webkit-transition: all 0.5s;
  -ms-transition: all 0.5s;
  -moz-transition: all 0.5s;
  -o-transition: all 0.5s;
  transition: all 0.5s;
  z-index: -1;
}

.display-overlay {
  left: 0px;
  background-color: rgba(0, 0, 0, 0.3);
  z-index: 3;
  cursor: pointer;
}


